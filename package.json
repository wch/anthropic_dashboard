    "@types/rstudio-shiny": "git+https://git@github.com/rstudio/shiny.git#v1.11.1",
    "autoprefixer": "^10.4.16",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "esbuild-plugin-tailwindcss": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "shadcn": "^3.0.0",
    "tailwindcss": "^4.1.12",
